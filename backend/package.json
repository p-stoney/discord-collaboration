{
  "name": "capstone-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "npm run build && node dist/main.js",
    "start:dev": "nest start --watch",
    "dev": "npm run build && nodemon --watch dist -e js --exec 'node dist/main.js'",
    "start:debug": "nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.12",
    "@discord-nestjs/core": "^5.3.14",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.4.5",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/platform-socket.io": "^10.4.4",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.4.4",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/cookie-parser": "^1.4.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-mongo": "^5.1.0",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.15.3",
    "express-session": "^1.18.0",
    "mongoose": "^8.5.3",
    "mongoose-autopopulate": "^1.1.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "ts-debounce": "^4.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.6",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.9",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.35",
    "@swc/jest": "^0.2.36",
    "@tsconfig/node18": "^18.2.4",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/passport-discord": "^0.1.13",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
